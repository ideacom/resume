/**index.wxss**/
.index-container{

}
.in-co-content{
  width: 100%;
}
.in-co-header{
  width: 100%;
  height: 400rpx;
  display: flex;
  flex-flow: column wrap;
  justify-content: center;
  align-items: center;
  background: #645E5D;
}
  .in-co-header view{
    /*width & height 只可以为奇数，否则会产生一像素偏移*/
    width: 221rpx;
    height: 221rpx;
    margin-bottom: 5%;
    border-radius: 111rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #BDBDBE;
  }
  .in-co-header image{
    /*width & height 只可以为偶数，否则会产生一像素偏移*/
    width: 200rpx;
    height: 200rpx;
    border-radius: 100rpx;
    background: #FFFFFF;
  }
  .in-co-header text{
    color: white;
  }
  .in-co-header .in-he-name{
    font-size: 45rpx;
  }
  .in-co-header .intention{
    font-size: 30rpx;
  }
.in-co-body{
  width: 100%;
  height: 600rpx;
  background: #645E5D;
}
  .in-bo-scroll{
    white-space: nowrap;
  }
    .in-sc-content{
      width: 500rpx;
      height: 300rpx;
      display: inline-block;
      margin-right: 1%;
      margin-top: 5%;
    }
    .in-sc-last{
      margin-right: 0%;
    }
    .in-sc-content image{
      width: 100%;
      height: 300rpx;
      background: #BDBDBE;
    }
  .in-de-contact{
  
  }
  .in-de-row{
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-start;
    align-items: center;
    margin-left: 2%;
  }
  .in-de-row image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 1%;
    background: white;
  }
  .in-de-row text{
    font-size: 28rpx;
    color: white;
  }

  